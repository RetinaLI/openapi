<div class="container">
  <div class='main-content'>
    <p>共搜索到24个结果：</p>
    <hr>
    <div class='service-list row'>
      <% for(var i = 0; i < pageInfo.list.length; i ++) {
            var ci = pageInfo.list[i];
            %>
        <div class="col-3 card-item">
          <div class='card'>
            <div class="card-body">
              <h5 class="card-title">
                <%=ci.name%>
              </h5>
              <p class="card-text">
                <%=ci.desc%>
              </p>
              <p class="text-center price">
                <%=ci.price%>
              </p>
              <div class='text-right card-btns'>
                <a href="/api/<%=ci.id%>/detail" class="card-link">详情</a>
              </div>
            </div>
          </div>
        </div>
        <% } %>
    </div>
    <div>
      <nav aria-label="Page navigation example">
        <ul class="pagination justify-content-end">
          <li class="page-item disabled">
            <a class="page-link" href="#" tabindex="-1">前一页</a>
          </li>
          <li class="page-item">
            <a class="page-link" href="<%=pageInfo.url.pathname%>?page=1">1</a>
          </li>
          <li class="page-item">
            <a class="page-link" href="<%=pageInfo.url.pathname%>?page=2">2</a>
          </li>
          <li class="page-item">
            <a class="page-link" href="<%=pageInfo.url.pathname%>?page=3">3</a>
          </li>
          <li class="page-item">
            <a class="page-link" href="<%=pageInfo.url.pathname%>?page=4">下一页</a>
          </li>
        </ul>
      </nav>
    </div>
  </div>
</div>